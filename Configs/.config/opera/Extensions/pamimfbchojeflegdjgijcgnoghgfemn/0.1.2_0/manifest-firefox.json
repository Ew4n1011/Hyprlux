{
  "name": "Kasm - Open In Isolation",
  "version": "0.1.2",
  "description": "Open links safely in Kasm Isolation",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "menu.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "browser_style": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "isolation@kasmweb.com"
    }
  }
}
